#include<stdio.h>


void main()
{
   int arr[100];
   int num;
   int i,j,temp;


   printf("Enter the number of elements : ");
   scanf("%d",&num);
   printf("Enter the elements \n");

   for(i=0;i<num;i++)
   {
      scanf("%d",&arr[i]);
   }

   for(i=0;i<num-1;i++)
   {
 
            if(arr[i]>arr[i+1])
                {
                    temp=arr[i];
                    arr[i]=arr[i+1];
                    arr[i+1]=temp;
                }
   }

   printf("Sorted array\n");
   for(i=0;i<num;i++)
   {
      printf("%d ",arr[i]);
   }

}
